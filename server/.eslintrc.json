{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-trailing-spaces": "off",
    "comma-dangle": "off",
    "object-curly-newline": "off",
    "linebreak-style": ["error", "windows"]
  }
}
